<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="model" default="$(find my_robot)/xacro/my_robot08.xacro"/>   
  <param name="robot_description" command="$(find xacro)/xacro $(arg model) --inorder"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch"/>

  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model my_robot" />

  <rosparam>
    my_robot:      
      arm1_joint_position_controller:
        type: effort_controllers/JointPositionController
        joint: arm1_joint
        pid: {p: 100.0, i: 0.01, d: 10.0}
  </rosparam>
  <node name="controller_spawner1" pkg="controller_manager"
        type="spawner" respawn="false"
        output="screen" ns="/my_robot"
        args="arm1_joint_position_controller"/>
  <rosparam>
    my_robot:      
      arm2_joint_position_controller:
        type: effort_controllers/JointPositionController
        joint: arm2_joint
        pid: {p: 100.0, i: 0.01, d: 10.0}
  </rosparam>
  <node name="controller_spawner2" pkg="controller_manager"
        type="spawner" respawn="false"
        output="screen" ns="/my_robot"
        args="arm2_joint_position_controller"/>

 

</launch>
